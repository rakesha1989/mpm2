<table class='table table-bordered table-hover table-condensed'>
  <thead>
    <tr>
      <th>Plan month</th>
      <th>Date</th>
      <th>Company</th>
      <th>Category</th>
    </tr>
  </thead>


  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.plan_month.strftime('%b %y') %></td>
        <td><%= meeting.date.strftime("%A %b %y %R") %></td>
        <td><%= meeting.company.name %></td>
        <td><%= meeting.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<button type="button" class="btn btn-info" id="print" onclick="myFunction()">Print</button><br/><br/>
<script type="text/javascript">
  function myFunction(){
    window.print()
  }
</script>
</div>