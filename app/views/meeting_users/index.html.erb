<p id="notice"><%= notice %></p>

<h1>Listing Meeting Users</h1>

<table>
  <thead>
    <tr>
      <th>Meeting</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_users.each do |meeting_user| %>
      <tr>
        <td><%= meeting_user.meeting_id %></td>
        <td><%= meeting_user.user_id %></td>
        <td><%= link_to 'Show', meeting_user %></td>
        <td><%= link_to 'Edit', edit_meeting_user_path(meeting_user) %></td>
        <td><%= link_to 'Destroy', meeting_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting user', new_meeting_user_path %>
